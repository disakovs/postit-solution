<%= render 'shared/error_messages', error_object: @user %>

<div class='well'>
  <%= form_for @user do |f| %>
    <%=f.label :username %>
    <%= f.text_field :username %>
    </br>
    <%=f.label :password %>
    <%= f.password_field :password %>
    </br>
    <%= f.submit(@user.new_record? ? 'Register' : 'Update Profile', class: 'btn btn-primary') %>

  <% end %>
</div>